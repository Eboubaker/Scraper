    "php-ffmpeg/php-ffmpeg": "^0.19.0",
    "tightenco/collect": "^8.81",
    "symfony/var-dumper": "v5.4.2",
    "vanilla/garden-cli": "^3.1",
    "ext-json": "*",
    "symfony/dom-crawler": "^6.0",
    "symfony/css-selector": "^6.0",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "license": "CC-BY-3.0",
  "authors": [
    {
      "name": "Eboubaker Bekkouche",
